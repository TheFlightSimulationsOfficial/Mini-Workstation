    <Name>Windows Mini Workstation</Name>
    <Version>1.2</Version>
    <OwnerType>ITAdmin</OwnerType>
    <Rank>52</Rank>
    <Notes></Notes>
  </PackageConfig>
  <Settings xmlns="urn:schemas-microsoft-com:windows-provisioning">
    <Customizations>
      <Common>
        <AccountManagement>
          <UserProfilePolicies>
            <EnableProfileManager>True</EnableProfileManager>
          </UserProfilePolicies>
        </AccountManagement>
        <Accounts>
          <ComputerAccount>
            <ComputerName>MiniWorkstation</ComputerName>
          </ComputerAccount>
        </Accounts>
        <DeviceManagement>
          <MemoryDump>
            <AllowCrashDump>Allowed</AllowCrashDump>
            <AllowLiveDump>Allowed</AllowLiveDump>
          </MemoryDump>
        </DeviceManagement>
        <EditionUpgrade>
          <ChangeProductKey>NRG8B-VKK3Q-CXVCJ-9G2XF-6Q84J</ChangeProductKey>
          <UpgradeEditionWithProductKey>NRG8B-VKK3Q-CXVCJ-9G2XF-6Q84J</UpgradeEditionWithProductKey>
        </EditionUpgrade>
        <Licensing>
          <AllowWindowsEntitlementReactivation>True</AllowWindowsEntitlementReactivation>
          <DisallowKMSClientOnlineAVSValidation>False</DisallowKMSClientOnlineAVSValidation>
        </Licensing>
        <OOBE>
          <Desktop>
            <EnableCortanaVoice>False</EnableCortanaVoice>
            <HideOobe>True</HideOobe>
          </Desktop>
        </OOBE>
        <Personalization>
          <DeployDesktopImage>F:\Windows Configuration Designer\Windows Mini Workstation\Mini-Workstation\wallpaper.png</DeployDesktopImage>
          <DeployLockScreenImage>F:\Windows Configuration Designer\Windows Mini Workstation\Mini-Workstation\lockscreen.png</DeployLockScreenImage>
        </Personalization>
        <Policies>
          <Accounts>
            <AllowAddingNonMicrosoftAccountsManually>Yes</AllowAddingNonMicrosoftAccountsManually>
          </Accounts>
          <ApplicationManagement>
            <AllowAllTrustedApps>Yes</AllowAllTrustedApps>
            <AllowAppStoreAutoUpdate>Disallowed</AllowAppStoreAutoUpdate>
            <AllowDeveloperUnlock>Yes</AllowDeveloperUnlock>
          </ApplicationManagement>
          <Camera>
            <AllowCamera>No</AllowCamera>
          </Camera>
          <FileExplorer>
            <AllowOptionToShowNetwork>True</AllowOptionToShowNetwork>
            <AllowOptionToShowThisPC>True</AllowOptionToShowThisPC>
          </FileExplorer>
          <Games>
            <AllowAdvancedGamingServices>Yes</AllowAdvancedGamingServices>
          </Games>
          <MemoryDump>
            <AllowCrashDump>Allowed</AllowCrashDump>
            <AllowLiveDump>Allowed</AllowLiveDump>
          </MemoryDump>
          <Power>
            <AllowHibernate>Not allowed</AllowHibernate>
          </Power>
          <Privacy>
            <AllowInputPersonalization>Yes</AllowInputPersonalization>
            <DisableAdvertisingId>Yes</DisableAdvertisingId>
          </Privacy>
          <Search>
            <AllowCloudSearch>Disabled</AllowCloudSearch>
            <AllowCortanaInAAD>False</AllowCortanaInAAD>
          </Search>
          <Start>
            <ForceStartSize>1</ForceStartSize>
            <HideFrequentlyUsedApps>Yes</HideFrequentlyUsedApps>
            <HideHibernate>No</HideHibernate>
            <HidePeopleBar>Yes</HidePeopleBar>
          </Start>
          <System>
            <AllowBuildPreview>Allowed</AllowBuildPreview>
            <AllowLocation>No</AllowLocation>
          </System>
          <TextInput>
            <AllowIMELogging>Yes</AllowIMELogging>
            <AllowInputPanel>Yes</AllowInputPanel>
          </TextInput>
          <Update>
            <AllowAutoUpdate>Turn off automatic updates</AllowAutoUpdate>
            <UpdateNotificationLevel>Default OS Windows Update notifications</UpdateNotificationLevel>
          </Update>
          <WindowsInkWorkspace>
            <AllowSuggestedAppsInWindowsInkWorkspace>Yes</AllowSuggestedAppsInWindowsInkWorkspace>
