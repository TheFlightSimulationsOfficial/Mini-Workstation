          </WindowsInkWorkspace>
          <WindowsLogon>
            <EnableFirstLogonAnimation>Yes</EnableFirstLogonAnimation>
            <HideFastUserSwitching>No</HideFastUserSwitching>
          </WindowsLogon>
        </Policies>
        <SMISettings>
          <AutoLogon>
            <Enable>ENABLE</Enable>
            <UserName>Workstation</UserName>
          </AutoLogon>
          <CrashDumpEnabled>Mini dump</CrashDumpEnabled>
          <DisableBootMenu>False</DisableBootMenu>
          <HideAllBootUI>False</HideAllBootUI>
          <HideBootLogo>False</HideBootLogo>
        </SMISettings>
      </Common>
    </Customizations>
